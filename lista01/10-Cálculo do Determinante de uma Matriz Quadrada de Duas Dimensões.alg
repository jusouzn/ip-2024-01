algoritmo "9 Cálculo de Delta na equação de Báskara"
var
    a, b, c, d, x : real

inicio
    escreva("Informe o número a: ")
    leia(a)
    escreva("Informe o número b: ")
    leia(b)
    escreva("Informe o número c: ")
    leia(c)
    escreva("Informe o número d: ")
    leia(d)

    x <-  a*d-b*c

    escreval("O VALOR DO DETERMINANTE E = ", x:6:2)


fimalgoritmo